{
  "name": "@suggesto/vue",
  "version": "1.0.0",
  "description": "Vue 3 composables and components for Suggesto feedback widget",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@suggesto/core": "1.0.0"
  },
  "devDependencies": {
    "vue": "^3.4.31",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^24.1.0"
  },
  "keywords": [
    "suggesto",
    "feedback",
    "widget",
    "vue",
    "vue3",
    "composable",
    "typescript"
  ],
  "author": "Niccol√≤ Banti",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
